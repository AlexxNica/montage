{
  "name": "montage",
  "version": "0.15.0",
  "description": "Build your next application with a browser based platform that really gets the web.",
  "repository": {
    "type": "git",
    "url": "https://github.com/montagejs/montage.git"
  },
  "main": "montage",
  "overlay": {
    "browser": {
      "main": "core/core",
      "redirects": {
        "montage": "core/core"
      },
      "mappings": {
      "mr": {
        "name": "mr",
        "location": "node_modules/mr"
      },
        "q": {
          "name": "q",
          "location": "packages/mr/packages/q"
        },
        "bluebird": {
          "name": "bluebird",
          "location": "node_modules/bluebird"
        }
      }
    }
  },
  "dependencies": {
    "collections": "~1.2.2",
    "frb": "~0.2.18",
    "htmlparser2": "~3.0.5",
    "mousse": "marchant/mousse.git#mousse-bluebird",
    "mr": "marchant/mr.git#mr-bluebird",
    "bluebird":"2.9.26"
  },
  "devDependencies": {
    "jshint": "~2.4.4",
    "montage-testing": "marchant/montage-testing.git#montage-testing-bluebird"
  },
  "scripts": {
    "test": "node test/node/node-spec.js",
    "jsdoc": "node ../montagejs.org/build/jsdoc/jsdoc.js -out $PWD/builds/jsdoc/ montage@npm-link"
  },
  "exclude": [
    "test",
    "tools"
  ]
}
